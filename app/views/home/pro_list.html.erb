
<h2>Project List </h2>

<table border=1 cellpadding=2 cellspacing=2>
  <tr>
    <th>Project title</th>
    <th>Project platform</th>
    <th>Client</th>
   <th>Assigned Developer(s)</th>
   
   
    
  </tr>

<% @projects.each do |project| %>
  <tr>
    <td><%= link_to(project.project_title,:contoller => 'home',:action=>'pro_desc',:id=> project.id) %></td>
    <td><%= project.project_platform %></td>
    <td><%= project.project_owner %></td>
	<td>
			<% for developer in project.developers -%>
				<%= developer.developer_name %><br />
			<% end -%>
		</td>

  </tr>
<% end %>

</table>
<%= will_paginate @projects %>
<br />


